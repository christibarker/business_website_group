<h1 class="photo_header">Contact Us</h1>

<!-- for form  place in the 2nd grid-->
<div class="container-fluid">
  <div class="row">
    <div id="left_margin" class="col-sm">
    </div>
    <div id="contact" class="col-sm">
      <div  class="form">
				<form action="/thanks" method="POST">
	  			<div class="form-group">
				    <label for="name">Name</label>
				    <input type="text" class="form-control" id="name" placeholder="Name">
	 				</div>
	   			<div class="form-group">
				    <label for="phone">Phone</label>
				    <input type="text" class="form-control" id="phone" placeholder="Phone">
				  </div>
	  			<div class="form-group">
			    	<label for="exampleInputEmail1">Email address</label>
			    	<input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Email">
			   	 <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
			  	</div>
	  			<div class="form-group">
				    <label for="m_m_name">Mother's Maiden Name</label>
				    <input type="text" class="form-control" id="m_m_name" placeholder="Mother's Maiden Name">
			  	</div>
	 				<div>
				  	<label type="services" for="pengiuns">Services Requested</label>
							<select name="mydropdown">
							<option value="blank"></option>
							<option value="one">Penguine Dress Up</option>
							<option value="two">Pengiun Awareness</option>
							<option value="three">Pengiun Parties</option>
							<option value="four">Penguin Strip Show</option>
							</select>
						</div>
	  				<div class="form-group">
					    <label for="message">Message</label>
					    <input type="message" class="form-control" id="message">
					  </div>
					  <button id="button" type="submit" class="btn btn-secondary">Submit Request</button>
				    <div id="right-margin"class="col-sm">
   					</div>
  				</div>
				</div>
			<div id="left_margin" class="col-sm">
    </div>
  </div>

